{
    "manifest_version" : 2,
    "name" : "CSS Box",
    "description" : "Enable Outline for all HTML Elements",
    "version" : "1.0",
    "browser_action" : {
        "default_icon" : "/images/16.png",
        "default_popup" : "popup.html"
    },
    "icons" : {
        "16" : "/images/16.png",
        "48" : "/images/48.png",
        "128" : "/images/128.png"
    },

    "permissions": [
        "activeTab"
      ],


    "content_scripts" : [{
        "matches" : ["<all_urls>"],
        "css" : ["style.css"]
    }]

}